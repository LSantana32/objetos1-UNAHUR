import vagones.*

describe "test de vagones" {
	const vp1= new VagonPasajeros(ancho=2,largo=10,tieneBanio=false)
	const vp2= new VagonPasajeros(ancho=2,largo=10,estaOrdenado=false)
	
	test "cantidad de personas de un vagon de pasajeros de ancho 2 y largo 10 ordenado"{
		assert.equals(80,vp1.cantPasajeros())
	}
	
	test "cantidad de personas de un vagon de pasajeros de ancho 4 y largo 10 "{
		assert.equals(85,vp2.cantPasajeros())
	}

}